	<script>
		$("document")
				.ready(
						function() {
							$("#back").click(function() {
								window.location.href = "calletter";
							});

							$("#acc").click(function() {
								
								if($("#ms").text()==="null" || $("#ms").text()==="0")
								{
								 $("#myModal3").modal('show');
								 $("#logoutid1").hide();
								}
							else
								{
								var id1 = $(this).prev('input').val();
								$.post("acceptinterview", {
									announceid : id1,
									value : "1"
								}).done(function() {
									window.setTimeout(function() {
										window.location.href = "calletter";
									}, 300);

								});
								}
							});
							
							$("#decline").click(function() {
								var id1 = $(this).prev('input').val();
								$.post("acceptinterview", {
									announceid : id1,
									value : "2"
								}).done(function() {
									window.setTimeout(function() {
										window.location.href = "calletter";
									}, 300);

								});

							});
							
							
							if ($(".status").text() === "3") 
							{
								$(".status").removeClass("label label-success").addClass("label label-danger").text("Not Applied");
							} 
							else if ($(".status").text() === "1")
							{
								$(".status").removeClass("label label-danger").addClass("label label-success").text("Applied");
								$("#acc").hide();
								$("#decline").show();
							}
							else if ($(".status").text() === "2")
							{
								$(".status").removeClass("label label-success").addClass("label label-danger").text("Decline");
								$("#acc").hide();
								$("#decline").hide();
							}
							else
								{
								
								}
							
							$(".interviewactive").addClass("active");
						});
	</script>
